{
  "name": "electronuxt",
  "version": "1.1.1",
  "description": "Create an Electron app that serves a Nuxt.js app",
  "main": "main.js",
  "repository": "git@github.com:detrohutt/electronuxt.git",
  "author": "Alexander Roberts <detrohutt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron-webpack dev",
    "build": "nuxt build",
    "start": "electron .",
    "pack": "npm run build && electron-packager ./ --overwrite"
  },
  "dependencies": {
    "cross-env": "^3.1.4",
    "electron": "^1.8.4",
    "exceljs": "^1.1.0",
    "font-awesome": "^4.7.0",
    "moment": "^2.22.0",
    "node-sass": "^4.8.3",
    "nuxt": "latest",
    "nuxt-class-component": "^1.2.0",
    "sass-loader": "^6.0.7",
    "sequelize": "^4.37.5",
    "sequelize-decorators": "^1.0.2",
    "sql.js": "^0.5.0",
    "vue-property-decorator": "^6.0.0",
    "vuex-persistedstate": "^2.5.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "electron-prebuilt-compile": "^1.8.4",
    "electron-webpack": "^1.13.0",
    "webpack-cli": "^2.0.13"
  },
  "config": {
    "nuxt": {
      "host": "localhost",
      "port": "3333"
    }
  }
